{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Next.js: pnpm run dev",
        "runtimeExecutable": "pnpm",
        "runtimeArgs": ["run", "dev"],
        "skipFiles": [
          "<node_internals>/**"
        ],
        "outFiles": [
          "${workspaceFolder}/.next/**/*.js"
        ]
      }
    ]
  }